{
  "manifestVersion": 1,
  "id": "webhint-devops",
  "publisher": "webhint",
  "version": "0.0.4",
  "name": "webhint",
  "description": "ADO extension for webhint",
  "public": false,
  "categories": [
    "Azure Pipelines"
  ],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "icons": {
    "default": "dist/static/images/extension-logo.png"
  },
  "contributions": [
    {
      "id": "webhint-task",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "tasks/webhint"
      }
    }
  ],
  "files": [
    {
      "path": "dist/static/images/extension-logo.png",
      "packagePath": "images/extension-logo.png",
      "addressable": true
    },
    {
      "path": "dist/static/images/screenshot-1.png",
      "packagePath": "images/screenshot-1.png",
      "addressable": true
    },
    {
      "path": "dist/static/images/screenshot-2.png",
      "packagePath": "images/screenshot-2.png",
      "addressable": true
    },
    {
      "path": "dist/static/images/task-icon.png",
      "packagePath": "tasks/webhint/icon.png",
      "addressable": true
    },
    {
      "path": "dist/task/task.json",
      "packagePath": "tasks/webhint/task.json",
      "addressable": true
    },
    {
      "path": "dist/task/execute-webhint.js",
      "packagePath": "tasks/webhint/execute-webhint.js",
      "addressable": true
    }
  ],
  "contributions": [
    {
      "id": "build-webhint-tab",
      "type": "ms.vss-build-web.build-results-tab",
      "description": "Embed webhint HTML result into a build tab",
      "targets": [
        "ms.vss-build-web.build-results-view"
      ],
      "properties": {
        "name": "webhint",
        "uri": "tab-build.html",
        "registeredObjectId": "registeredEnvironmentObject",
        "supportsTasks": [
          "12c1d9d0-f9f1-428a-9023-2ba3965a5178"
        ],
        "dynamic": true
      },
      "includes": [
        "ms.vss-releaseManagement-web.release-service-data-external"
      ]
    },
    {
      "id": "release-stage-webhint-tab",
      "type": "ms.vss-web.tab",
      "description": "Embed webhint HTML result into a release stage tab",
      "targets": [
        "ms.vss-releaseManagement-web.release-environment-editor-tab"
      ],
      "properties": {
        "name": "webhint",
        "uri": "tab-release.html",
        "registeredObjectId": "registeredEnvironmentObject",
        "supportsTasks": [
          "12c1d9d0-f9f1-428a-9023-2ba3965a5178"
        ],
        "dynamic": true
      },
      "includes": [
        "ms.vss-releaseManagement-web.release-service-data-external"
      ]
    },
    {
      "id": "webhint",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "tasks/webhint"
      }
    }
  ]
}
